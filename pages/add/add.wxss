.view-content{
  margin-top: 19rpx;
  background: #fff;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
  width: 100%;
}
.item-list{
  display: flex;
  align-items: center;
  padding-left: 32rpx;
  box-sizing: border-box;
  width: 100%; 
  height: 90rpx;
}
.item-content{
  display: flex;
  align-items: center;
  width: 100%;
  height: 100%;
  border-bottom: 1rpx solid #e5e5e5;
}
.item-name{
  font-size: 34rpx;
  color: #000000;
  min-width: 133rpx;
}
.item-name text{
  color: #f4333c;
}
.item-input{
  margin-left: 45rpx;
  font-size: 34rpx;
}


.btn{
  margin-top: 59rpx;
  width: 100%;
  height: 88rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.btn-click{
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 88rpx;
  font-size: 36rpx;
  background: #1f8de3;
  color: #fff;
  border-radius: 10rpx;
}